# .env.example
# ---------------------
# Environment configuration for Veroxe MLOps Platform
# Rename this file to `.env` and fill in the actual values.

##########################
# Google Cloud Platform
##########################

GCP_PROJECT_ID=your-gcp-project-id
GCP_REGION=us-central1

##########################
# Google Cloud Storage (GCS)
##########################

GCS_BUCKET=ml-dataset-veroxe
GCS_UPLOAD_PREFIX=uploads/
GCS_PROCESSED_PREFIX=uploads/processed/
GCS_DOWNLOAD_PREFIX=downloads/
GCS_ARCHIVE_PREFIX=archive/

##########################
# BigQuery
##########################

BQ_DATASET=veroxe_dataset
BQ_TABLE=data_validation

##########################
# Pub/Sub
##########################

PUBSUB_TOPIC=data-upload-topic
PUBSUB_SUBSCRIPTION=data-upload-sub

##########################
# Vertex AI
##########################

VERTEX_LOCATION=us-central1
MODEL_NAME=veroxe-model
ENDPOINT_NAME=veroxe-endpoint

##########################
# API & Inference
##########################

INFERENCE_URL=https://your-cloudrun-inference-url
API_KEY=your-api-key-here

##########################
# OAuth (for web-ui auth)
##########################

GOOGLE_OAUTH_CLIENT_ID=your-google-oauth-client-id
GOOGLE_OAUTH_CLIENT_SECRET=your-google-oauth-client-secret

##########################
# Local Development
##########################

PORT=8000
LOG_LEVEL=debug
